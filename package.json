{
  "name": "SuperAda-Admin",
  "version": "1.0.0",
  "description": "Admin frontend for the SuperAda event",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "start-nobrowser": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rm -rf dist && webpack --config webpack/webpack.config.prod.js  && cp -a assets/ dist/",
    "deploy": "rm -rf dist && webpack --config webpack/webpack.config.prod.js  && cp -a assets/ dist/ && surge ./dist -d superada.surge.sh",
    "prod": "serve ./dist -s",
    "lint": "eslint src",
    "ngrok": "ngrok http -region eu 8080",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/futurice/SuperAda-Admin.git"
  },
  "keywords": [
    "react",
    "redux",
    "async"
  ],
  "author": "futurice",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/futurice/SuperAda-Admin/issues"
  },
  "homepage": "https://github.com/futurice/SuperAda-Admin#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-styled-components": "0.0.3-0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^18.0.0",
    "react-test-renderer": "^15.4.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0-rc.0"
  },
  "dependencies": {
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.7",
    "offline-plugin": "^4.5.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-image-fallback": "^4.0.1",
